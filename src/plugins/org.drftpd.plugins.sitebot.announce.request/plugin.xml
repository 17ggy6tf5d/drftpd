<?xml version="1.0" ?>
<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 1.0" "http://jpf.sourceforge.net/plugin_1_0.dtd">
<plugin id="org.drftpd.master.plugins.sitebot.announce.request" version="3.4.2">
	<doc>
		<doc-text>(Sitebot) - This plugin provides Request announcements for Sitebot</doc-text>
	</doc>
	<requires>
        <import plugin-id="org.drftpd.master.commands.request" />
		<import plugin-id="org.drftpd.master.plugins.sitebot" />
	</requires>
	<runtime>
		<library id="org.drftpd.master.plugins.sitebot.announce.request" path="classes/" type="code" />
	</runtime>
	<extension plugin-id="org.drftpd.master.plugins.sitebot" point-id="Announce" id="RequestAnnounce">
		<parameter id="Class" value="org.drftpd.master.plugins.sitebot.announce.request.RequestAnnouncer" />
	</extension>
</plugin>
