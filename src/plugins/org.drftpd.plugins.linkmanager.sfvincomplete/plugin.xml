<?xml version="1.0" ?>
<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 1.0" "http://jpf.sourceforge.net/plugin_1_0.dtd">
<plugin id="org.drftpd.master.plugins.linkmanager.sfvincomplete" version="3.4.2">
	<doc>
		<doc-text> This plugin provides SFVIncomplete to the Link Manager</doc-text>
	</doc>
    <attributes>
		<attribute id="DefaultBuild" value="false" />
		<attribute id="MustBuild" value="false" />
	</attributes>
	<requires>
		<import plugin-id="org.drftpd.master.commands.zipscript" />
		<import plugin-id="org.drftpd.master.plugins.linkmanager" exported="true" />
		<import plugin-id="master" exported="true" reverse-lookup="true" />
	</requires>

	<runtime>
		<library id="org.drftpd.master.plugins.linkmanager.types.sfvincomplete" path="classes/" type="code">
			<export prefix="*" />
		</library>
	</runtime>
	
	<extension plugin-id="org.drftpd.master.plugins.linkmanager" point-id="LinkType" id="SFVIncomplete">
		<parameter id="ClassName" value="org.drftpd.master.plugins.linkmanager.types.sfvincomplete.SFVIncomplete"/>
		<parameter id="TypeName" value="SFVIncomplete"/>
	</extension>	
	
	<extension plugin-id="master" point-id="Plugin" id="SFVIncompleteManager">
		<parameter id="Class" value="org.drftpd.master.plugins.linkmanager.types.sfvincomplete.SFVIncompleteManager"/>
	</extension>
	
</plugin>
